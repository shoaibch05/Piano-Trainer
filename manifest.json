{
    "manifest_version": 3,
    "name": "Piano Trainer",
    "version": "1.0",
    "description": "Play virtual Piano and learn with songs",
    "action": {
      "default_title": "Open Piano Trainer"
    },
    "background": {
      "service_worker": "background.js"
    },
    "permissions": ["tabs"],
    "icons": {
      "16": "/icons/icon48.png",
      "48": "/icons/icon48.png",
      "128": "/icons/icon48.png"
    }
  }